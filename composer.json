{
  "require": {
    "catfan/medoo": "^1.5",
    "guzzlehttp/guzzle": "^6.5",
    "ext-mbstring": "*",
    "ext-libxml": "*",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-intl": "*"
  },
  "scripts": {
    "csfix": "phpcbf --standard=PSR12 --colors --ignore=./vendor,./rambouillet/unyson/PluginSettings.php,./bootstrap.php ./",
